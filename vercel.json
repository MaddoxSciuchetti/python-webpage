{
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python@3.0.0" }
  ],
  "installCommand": "pip3 install -r requirements.txt",
  "buildCommand": "python3 manage.py collectstatic --noinput",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
